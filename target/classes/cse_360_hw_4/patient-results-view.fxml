<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.text.Text?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="cse_360_hw_4.Controllers.PatientResultsController"
            prefHeight="400.0" prefWidth="600.0">

    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Text layoutX="20.0" layoutY="80.0" text="The total Agatston CAC score"/>
    <TextField fx:id="totalAgatston" editable="false" layoutX="150.0" layoutY="70.0" prefWidth="200.0"/>

    <Text layoutX="20.0" layoutY="120.0" text="Vessel level Agatston CAC score"/>

    <Text layoutX="20.0" layoutY="160.0" text="LM:"/>
    <TextField fx:id="LM" editable="false" layoutX="150.0" layoutY="150.0" prefWidth="200.0"/>

    <Text layoutX="20.0" layoutY="200.0" text="LAD:"/>
    <TextField fx:id="LAD" editable="false" layoutX="150.0" layoutY="190.0" prefWidth="200.0"/>

    <Text layoutX="20.0" layoutY="240.0" text="LCX:"/>
    <TextField fx:id="LCX" editable="false" layoutX="150.0" layoutY="230.0" prefWidth="200.0"/>

    <Text layoutX="20.0" layoutY="280.0" text="RCA:"/>
    <TextField fx:id="RCA" editable="false" layoutX="150.0" layoutY="270.0" prefWidth="200.0"/>

    <Text layoutX="20.0" layoutY="320.0" text="PDA:"/>
    <TextField fx:id="PDA" editable="false" layoutX="150.0" layoutY="310.0" prefWidth="200.0"/>

</AnchorPane>